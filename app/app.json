{
  "expo": {
    "name": "RFC-App",
    "slug": "RFC-App",
    "version": "5.0.0",
    "orientation": "portrait",
    "icon": "./App/presentation/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/presentation/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./App/presentation/assets/favicon.png"
    },
    "scheme": "rfc-app",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow RFC App to use your location."
        }
      ],
      "expo-font",
      "expo-secure-store",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDiNmgWgB0CGoM1EG6CT846M83bpRdso-Q"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "RFCApp",
              "host": "checkout",
              "pathPrefix": "/congrats"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "versionCode": 6,
      "googleServicesFile": "./google-services.json",
      "package": "com.arielcito.RFCApp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "INTERNET"
      ]
    },
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyDiNmgWgB0CGoM1EG6CT846M83bpRdso-Q"
      },
      "bundleIdentifier": "com.arielcito.RFCApp",
      "buildNumber": "6.0.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      }
    },
    "extra": {
      "eas": {
        "projectId": "3f2c002c-cc79-46b0-8b8d-d24f2126b8ca"
      }
    },
    "newArchEnabled": true
  }
}
