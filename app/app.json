{
  "expo": {
    "name": "RFC-App",
    "slug": "RFC-App",
    "version": "6.0.0",
    "orientation": "portrait",
    "icon": "./App/presentation/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/presentation/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./App/presentation/assets/favicon.png"
    },
    "scheme": "rfc-app",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow RFC App to use your location."
        }
      ],
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0",
            "useFrameworks": "static",
            "flipper": false,
            "pods": {
              "GoogleMaps": {
                "modular_headers": true
              },
              "BoringSSL-GRPC": {
                "compiler-flags": "-fno-objc-msgsend-selector-stubs"
              }
            }
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics"
    ],
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDiNmgWgB0CGoM1EG6CT846M83bpRdso-Q"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "RFCApp",
              "host": "checkout",
              "pathPrefix": "/congrats"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "versionCode": 6,
      "googleServicesFile": "./google-services.json",
      "package": "com.arielcito.RFCApp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./App/presentation/assets/adaptative-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyDiNmgWgB0CGoM1EG6CT846M83bpRdso-Q"
      },
      "bundleIdentifier": "com.arielcito.RFCApp",
      "buildNumber": "6.0.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos.",
        "NSLocationWhenInUseUsageDescription": "La aplicación necesita acceso a tu ubicación para mostrar lugares cercanos",
        "NSLocationAlwaysUsageDescription": "La aplicación necesita acceso a tu ubicación para funcionalidades en segundo plano",
        "NSCameraUsageDescription": "La aplicación necesita acceso a la cámara para tomar fotos",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "extra": {
      "eas": {
        "projectId": "3f2c002c-cc79-46b0-8b8d-d24f2126b8ca"
      }
    },
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3f2c002c-cc79-46b0-8b8d-d24f2126b8ca",
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "owner": "arielcito"
  }
}
