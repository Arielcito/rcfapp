    "react-native-dotenv": "^3.4.11",
    "react-native-svg-transformer": "^1.5.0",
    "tailwindcss": "^3.3.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-maps",
          "react-native-element-dropdown"
        ],
        "listUnknownPackages": false
      }
    }
  }
